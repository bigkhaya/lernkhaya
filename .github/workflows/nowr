const getUserChoice = userInput => {
    userInput = userInput.toLowerCase() ;

    if (userInput === 'rock' || userInput ==='paper' || userInput=== 'scissors' || userInput==='bomb') {
        return userInput ;
    }else {
        console.log('error');
    }

}
const computerChoice = () => {
    computer = Math.floor(Math.random()*4);
    if  (computer=== 0) {
        return 'rock' ;
    }else if (computer ===1){
        return 'paper';
    }else {
        return 'scissors' ;
    }
    
}    

